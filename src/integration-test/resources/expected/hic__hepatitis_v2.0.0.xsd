<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://nhic.co.uk/hepatitis/2.0.0" xmlns="http://nhic.co.uk/hepatitis/2.0.0">
